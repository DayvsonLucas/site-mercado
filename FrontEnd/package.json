{
  "name": "sitemercado",
  "author": "Dayvson Silva",
  "email": "contato@sitemercado.com.br",
  "licence": "MIT",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-extend-native": "off",
      "object-curly-newline": [
        "error",
        {
          "ObjectPattern": {
            "multiline": true
          },
          "ImportDeclaration": {
            "multiline": true
          },
          "ExportDeclaration": {
            "multiline": true
          }
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.9.5",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "4.0.0-alpha.45",
    "@material-ui/styles": "4.9.0",
    "axios": "^0.19.0",
    "change-case": "^4.1.1",
    "chart.js": "^2.8.0",
    "clsx": "^1.0.4",
    "env-cmd": "^10.0.1",
    "formik": "^2.1.4",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "notistack": "^0.9.5",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.11.0",
    "react-dropzone": "^11.2.4",
    "react-feather": "^2.0.8",
    "react-file-base64": "^1.0.3",
    "react-helmet": "^5.2.1",
    "react-input-mask": "^2.0.4",
    "react-perfect-scrollbar": "^1.5.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-multi": "^0.1.12",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "validate.js": "^0.13.1",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "eslint": "6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "typescript": "^3.6.4"
  }
}